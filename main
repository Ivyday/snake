#include <iostream>
#include <iomanip>
using namespace std;
int main() {
	char arr[10][10];
	for (int i = 0; i < 10; i++) {
		for (int j = 0; j < 10; j++) {
			if (i == 0 || i == 9 || j == 0 || j == 9)
				cout <<setw(2)<< "*";
			else
				cout << setw(2) << " ";
			
		}
		cout << endl;
	}
  //邊框
}
